<div id="title-form" title="Create a Survey">

  <form style="padding-top:18px" id="title-input" enctype="multipart/form-data">
    <fieldset>
      <label for="title">Title:</label>
      <input type="text" id="title" name="title" placeholder="Enter the title of your survey here" size="50" required><br>
      <label for="image" style="padding-top:10px">Survey Icon:</label>
      <input type="file" id="survey_image" name="image">
      <label for="make_private" style="padding-top:10px">Make survey private?</label>
      <input type="checkbox" id="make_private" name="make_private" value="yes"> Yes<br>
  </form>

</div>